{
  "name": "pagevamp/laravel-image-processor",
  "description": "Package for uploading / resizing images into disk. Tested with s3/local & laravel 5.8",
  "type": "library",
  "require": {
    "laravel/framework": "5.*",
    "intervention/image": "^2.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12",
    "orchestra/testbench": "^3.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ujwal Dhakal",
      "email": "kevin.ujwal@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pagevamp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pagevamp\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pagevamp\\Providers\\ProcessorServiceProvider"
      ]
    }
  }
}
